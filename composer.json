{
  "name": "minimal/framework",
  "description": "The Minimal Framework.",
  "keywords": [
    "framework",
    "minimal",
    "mvc"
  ],
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Julien Duseyau",
      "email": "julien.duseyau@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.0.0",
    "minimal/assets": "v0.1.2",
    "minimal/benchmark": "v0.1.2",
    "minimal/cli": "v0.1.1",
    "minimal/database": "v0.1.3",
    "minimal/html": "v0.1.2",
    "minimal/minimal": "v0.1.11",
    "minimal/paths": "v0.1.3",
    "minimal/presenters": "v0.1.1",
    "minimal/translation": "v0.1.2",
    "minimal/views": "v0.1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "symfony/var-dumper": "3.3.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    },
    "files": [
      "helpers/helpers.php"
    ]
  },
  "scripts": {
    "post-create-project-cmd": [
      "php -r \"rename('config/_development.php', 'config/development.php');\"",
      "php -r \"rename('config/_production.php', 'config/production.php');\"",
      "php -r \"rename('config/_environment.php', 'config/environment.php');\""
    ]
  }
}
